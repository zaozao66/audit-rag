{
  "development": {
    "embedding_model": {
      "provider": "openai",
      "model_name": "text-embedding-v4",
      "api_key": "YOUR_DEVELOPMENT_API_KEY_HERE",
      "endpoint": "https://dashscope.aliyuncs.com/compatible-mode/v1",
      "dimension": 1024,
      "ssl_verify": true
    },
    "rerank_model": {
      "provider": "aliyun",
      "model_name": "qwen3-rerank",
      "api_key": "YOUR_DEVELOPMENT_API_KEY_HERE",
      "endpoint": "https://dashscope.aliyuncs.com/compatible-api/v1",
      "dimension": 1024,
      "ssl_verify": true
    },
    "llm_model": {
      "provider": "deepseek",
      "model_name": "deepseek-chat",
      "api_key": "YOUR_DEEPSEEK_API_KEY_HERE",
      "endpoint": "https://api.deepseek.com/v1",
      "temperature": 0.7,
      "max_tokens": 2000,
      "ssl_verify": true
    },
    "chunking": {
      "chunk_size": 512,
      "overlap": 50,
      "chunker_type": "smart"
    },
    "search": {
      "top_k": 5,
      "similarity_threshold": 0.5
    },
    "conversation": {
      "max_messages": 24,
      "ttl_minutes": 120
    },
    "vector_store_path": "./data/vector_store_text_embedding"
  },
  "production": {
    "embedding_model": {
      "provider": "openai",
      "model_name": "bge",
      "api_key": "YOUR_PRODUCTION_API_KEY_HERE",
      "endpoint": "https://your-production-embedding.com/v1",
      "dimension": 1024,
      "ssl_verify": false
    },
    "rerank_model": {
      "provider": "openai",
      "model_name": "bge-reranker-large",
      "api_key": "YOUR_PRODUCTION_API_KEY_HERE",
      "endpoint": "https://your-production-rerank.com/v1",
      "dimension": 1024,
      "ssl_verify": false
    },
    "llm_model": {
      "provider": "deepseek",
      "model_name": "deepseek-chat",
      "api_key": "YOUR_DEEPSEEK_API_KEY_HERE",
      "endpoint": "https://api.deepseek.com/v1",
      "temperature": 0.7,
      "max_tokens": 2000,
      "ssl_verify": false
    },
    "chunking": {
      "chunk_size": 512,
      "overlap": 50,
      "chunker_type": "smart"
    },
    "search": {
      "top_k": 5,
      "similarity_threshold": 0.5
    },
    "conversation": {
      "max_messages": 24,
      "ttl_minutes": 120
    },
    "vector_store_path": "./data/vector_store_text_embedding"
  },
  "default_env": "development",
  "current_env": "development"
